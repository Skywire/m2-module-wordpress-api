{
    "name": "skywire/wordpressapi",
    "description": "Wordpress integration",
    "license": 	"OSL-3.0",
    "require": {
        "php": "^7.0|^7.1|^7.2|^7.3|^7.4|^8",
        "magento/magento-composer-installer": "*",
        "skywire/testframework":"*",
        "guzzlehttp/guzzle": "^7.4"
    },
    "type": "magento2-module",
    "repositories": {
        "festframework":{
            "type": "vcs",
            "url": "https://github.com/Skywire/m2-test-framework.git"
        }
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Skywire\\WordpressApi\\": "src"
        }
    },
    "config": {
        "allow-plugins": true
    }
}
